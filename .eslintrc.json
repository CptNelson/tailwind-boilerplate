{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "plugins": ["radar"],
  "extends": ["eslint:recommended", "plugin:radar/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-var": "warn",
    "no-empty-function": "off",
    "no-empty": "off"
  }
}